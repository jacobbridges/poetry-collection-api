  "reference": "http://www.poetryfoundation.org/poem/241890",
  "region": "U.S., Midwestern",
  "text": [
    "sun",
    "sunwarm",
    "sunwarm on back",
    "sunwarm on back legs",
    "sunwarm on back legs loosens",
    "my heart",
    "my heart beats",
    "my heart beats faster",
    "in sunwarm my heart beats faster",
    "I flex",
    "I flex legs",
    "I flex legs loose with sunwarm",
    "I drink dew from dripping leaves",
    "I beat",
    "flex",
    "crouch",
    "leap!",
    "What am I?",
    "(grasshopper)",
    "\ufeff"
  ],
  "title": "Morning Warming",
  "year": ""
}